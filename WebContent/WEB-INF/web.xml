<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>CozyKRoo_2</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
   
  <servlet>
  	<servlet-name>MembersFrontController</servlet-name>
  	<servlet-class>com.koreait.app.members.MembersFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>MembersFrontController</servlet-name>
	<url-pattern>*.me</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>BoardsFrontController</servlet-name>
  	<servlet-class>com.koreait.app.boards.BoardsFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>BoardsFrontController</servlet-name>
	<url-pattern>*.bo</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>DiaryFrontController</servlet-name>
  	<servlet-class>com.koreait.app.diary.DiaryFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>DiaryFrontController</servlet-name>
	<url-pattern>*.di</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ShareFrontController</servlet-name>
  	<servlet-class>com.koreait.app.share.ShareFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ShareFrontController</servlet-name>
	<url-pattern>*.sh</url-pattern>
  </servlet-mapping>
  
  
</web-app>